msgid "markup"
msgstr "markup"

msgctxt "TC_CAT_MISC"
msgid "misc"
msgstr "misc"

msgctxt "TC_CAT_PRETTYPRINT"
msgid "print"
msgstr "imprimer"

msgctxt "TC_LABEL_COL"
msgid "column"
msgstr "colonne"

msgctxt "TC_LABEL_FILE"
msgid "file"
msgstr "fichier"

msgctxt "TC_LABEL_LANG"
msgid "lang"
msgstr "lang"

